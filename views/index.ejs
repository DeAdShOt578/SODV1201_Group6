
   <html>
  <head>
    <%- include ('header.ejs') %>

  <body>
   <% users.forEach(function(user){ %>
  
  <section class="main-bg">
		<div class="container">
			<div class="row">
				
				<div class="col-sm-6 col-sm-offset-3">
					<h1 class="text-center">Update Property</h1>
					<form action="/updatepropertysave" method="post">
						<h4 class="modal-title text-center" id="myModalLabel">Update Details</h4>
						<input type="hidden" name="pid" value="<%= user.propertyid %>" />
						<hr>
						<div class="row">
							<div class="col-sm-6">
								<div class="form-group">
									<label>Select Type</label>
									<select id="propertytype" name="propertytype" class="form-control">
										<option><%= user.propertytype %></option>
										<option>Apartment</option>
										<option>Bungalow</option>
										<option>Cabin</option>
										<option>House</option>
									</select>
								</div>
							</div>

							<div class="col-sm-6">
								<div class="form-group">
									<label>Enter Property Description</label>
									<input type="text" class="form-control" value="<%= user.propertydesc %>" name="propertydesc" id="desc" />
								</div>
							</div>

							<div class="col-sm-6">
								<div class="form-group">
									<label>Select Area</label>
									<select id="area" class="form-control" name="propertyarea">
										<option><%= user.area %></option>
										<option>Calgary</option>
										<option>Missisaga</option>
										<option>Markham</option>
										<option>Brampton</option>
									</select>
								</div>
							</div>

							<div class="col-sm-6">
								<div class="form-group">
									<label>Address</label>
									<input type="text" class="form-control" value="<%= user.propertyaddress %>" name="propertyaddress" id="rent" />
								</div>
							</div>

							<div class="col-sm-6">
								<div class="form-group">
									<label>Neighbourhood</label>
									<input type="text" class="form-control" value="<%= user.neighbourhood %>" name="neighbourhood" id="rent" />
								</div>
							</div>

							<div class="col-sm-6">
								<div class="form-group">
									<label>Sq. Feet</label>
									<input type="number" class="form-control" value="<%= user.sqfeet %>" name="sqfeet" id="rent" />
								</div>
							</div>

							<div class="col-sm-6">
								<div class="form-group">
									<label>Parking Garage?</label>
									<select id="area" class="form-control" name="parking">
										<option><%= user.parking %></option>
										<option>Yes</option>
										<option>No</option>

									</select>
								</div>
							</div>

							<div class="col-sm-6">
								<div class="form-group">
									<label>Reachhable by public transport?</label>
									<select id="area" class="form-control" name="reachable">
										<option><%= user.reachable %></option>
										<option>Yes</option>
										<option>No</option>

									</select>
								</div>
							</div>

							<div class="col-sm-6">
								<div class="form-group">
									<label>Property Rent</label>
									<input type="text" class="form-control" value="<%= user.propertyrent %>" name="propertyrent" id="rent" />
								</div>
							</div>
							<div class="col-sm-6">
								<div class="form-group">
									<label>Phone No.</label>
									<input type="text" class="form-control" value="<%= user.phoneno %>" name="contact" id="phone" />
								</div>
							</div>
							<div class="col-sm-12">
								<button type="submit" class="btn btn-info">Update</button>
							</div>
						</div>
					</form>
				</div>
			</div>
		</div>
	</section>
    <ul>
 
</ul>
<% })%>
    <%- include('footer.ejs') %>
  </body>
</html>