<%- include ('header.ejs') %>	
	
	<!---- main background----->
	<section class="main-bg1">
		<div class="container">
			<div class="row">
				<div class="col-md-12">
				<h1>List of Properties</h1>
				
				<table class="table table-striped" border="1">
					<tr>
						<th>Id</th>
						<th>Type</th>
						<th>Description</th>
						<th>Area</th>
						<th>Rent</th>
						<th>Parking</th>
						<th>Reachable</th>
						<th>Neighbourhood</th>
						<th>Contact No.</th>
					</tr>
					<% for(var i=0; i < data.length; i++) { %>
					   <tr>
						 <td><%= data[i].propertyid %></td>
						 <td><%= data[i].propertytype %></td>
						 <td><%= data[i].propertydesc %></td>
						 <td><%= data[i].area %></td>
						 <td><%= data[i].propertyrent %></td>
						 <td><%= data[i].parking %></td>
						 <td><%= data[i].reachable%></td>
						 <td><%= data[i].neighbourhood %></td>
						 <td><%= data[i].phoneno %></td>

					   </tr>
					<% } %>
					</table>
				</div>
			</div>
		</div>
	</section>
	
	
	
	
	
	<!----footer ----->
	
	<%- include ('footer.ejs') %>	

